{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)", 
      "dest": "/src/api/index.js"
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html" 
    }
  ]
}